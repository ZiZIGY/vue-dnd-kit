{
  "name": "@vue-dnd-kit/components",
  "version": "0.2.37",
  "description": "Components for Vue DnD Kit",
  "author": "ZiZIGY",
  "license": "MIT",
  "type": "module",
  "bin": "./dist/cli.js",
  "keywords": [
    "vue",
    "dnd",
    "drag",
    "drop",
    "drag and drop",
    "drag and drop library",
    "vue dnd",
    "vue drag and drop",
    "vue drag and drop library",
    "vue dnd kit",
    "vue dnd kit components",
    "dnd-kit",
    "dnd-kit components",
    "vue-dnd-kit",
    "vue-dnd-kit components",
    "@vue-dnd-kit/components"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zizigy/vue-dnd-kit.git",
    "directory": "packages/components"
  },
  "scripts": {
    "dev": "vite build --watch",
    "start": "vite build --watch",
    "build": "rimraf dist && vite build && tsup",
    "build:cli": "tsup",
    "test": "vitest run",
    "lint": "eslint src --ext .ts,.vue",
    "prepublishOnly": "npm run build"
  },
  "main": "./dist/vue-dnd-kit-components.es.js",
  "module": "./dist/vue-dnd-kit-components.es.js",
  "unpkg": "./dist/vue-dnd-kit-components.umd.js",
  "types": "./dist/types/index.d.ts",
  "browser": {
    "./sfc": "./src/components/"
  },
  "files": [
    "README.md",
    "CHANGELOG.md",
    "LICENSE",
    "dist",
    "src/components/*.vue",
    "templates"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/vue-dnd-kit-components.es.js",
      "require": "./dist/vue-dnd-kit-components.cjs.js"
    },
    "./sfc": "./src/components/",
    "./styles": "./dist/style.css"
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "commander": "^14.0.0",
    "fs-extra": "^11.3.0"
  },
  "peerDependencies": {
    "@vue-dnd-kit/core": "^0.5.0",
    "@vueuse/core": "^10.0.0 || ^13.0.0",
    "vue": "^3.3.0 || ^3.4.0 || ^3.5.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^18.11.18",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vueuse/core": "^13.1.0",
    "eslint": "^8.38.0",
    "rimraf": "^3.0.2",
    "tsup": "^8.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^4.5.0",
    "vitepress": "^1.6.3",
    "vue": "^3.5.13",
    "vue-tsc": "^1.8.27"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
